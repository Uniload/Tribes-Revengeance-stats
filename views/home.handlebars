	Hello world


	<h2 name="top">Top players</h2>
	<table class="table table-bordered">
		<tr>
			<th>Name</th>
			<th>Kills</th>
			<th>Deaths</th>
			<th>Style</th>
			<th>Flag captures</th>
			<th>Time played</th>
			<th>Last seen</th>
		</tr>
	{{#each data}}
		<tr>
			<td><a href="/player/{{urlencode _id}}">{{_id}}</a></td>
			<td>{{kills}}</td>
			<td>{{deaths}}</td>
			<td>{{style}}</td>
			<td></td>
			<td>{{minutesonline}} minutes</td>
			<td>{{showMoment lastseen}}</td>
		</tr>
	{{/each}}
	</table>
</div>